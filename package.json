{
  "name": "journal-index-integration",
  "version": "1.0.0",
  "description": "Serverless configuration for Journal Index Integration Lambda functions",
  "main": "index.js",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:function": "serverless deploy function",
    "remove": "serverless remove",
    "remove:dev": "serverless remove --stage dev",
    "remove:staging": "serverless remove --stage staging",
    "remove:prod": "serverless remove --stage prod",
    "logs": "serverless logs",
    "logs:creator": "serverless logs -f creatorCommand",
    "logs:importer": "serverless logs -f importCommand",
    "logs:health": "serverless logs -f healthCheck",
    "logs:dev": "serverless logs --stage dev",
    "logs:staging": "serverless logs --stage staging",
    "logs:prod": "serverless logs --stage prod",
    "invoke": "serverless invoke",
    "invoke:creator": "serverless invoke -f creatorCommand",
    "invoke:importer": "serverless invoke -f importCommand",
    "invoke:health": "serverless invoke -f healthCheck",
    "invoke:dev": "serverless invoke --stage dev",
    "invoke:staging": "serverless invoke --stage staging",
    "invoke:prod": "serverless invoke --stage prod",
    "offline": "serverless offline",
    "offline:dev": "serverless offline --stage dev",
    "offline:staging": "serverless offline --stage staging",
    "test": "mvn test",
    "test:dev": "mvn test -Dspring.profiles.active=dev",
    "test:staging": "mvn test -Dspring.profiles.active=staging",
    "test:prod": "mvn test -Dspring.profiles.active=prod",
    "build": "mvn clean package",
    "build:skip-tests": "mvn clean package -DskipTests",
    "build:dev": "mvn clean package -Dspring.profiles.active=dev",
    "build:staging": "mvn clean package -Dspring.profiles.active=staging",
    "build:prod": "mvn clean package -Dspring.profiles.active=prod",
    "install:java": "mvn clean install",
    "clean": "mvn clean",
    "validate": "serverless validate",
    "profile:dev": "./select-profile.sh dev",
    "profile:staging": "./select-profile.sh staging",
    "profile:prod": "./select-profile.sh prod",
    "profile:select": "./select-profile.sh",
    "run:dev": "mvn spring-boot:run -Dspring.profiles.active=dev",
    "run:staging": "mvn spring-boot:run -Dspring.profiles.active=staging",
    "run:prod": "mvn spring-boot:run -Dspring.profiles.active=prod"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "journal",
    "oai",
    "mysql",
    "sqs"
  ],
  "author": "Teckiz",
  "license": "MIT",
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-offline": "^12.3.0",
    "serverless-plugin-warmup": "^8.2.1",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-python-requirements": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/journal-index-integration.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/journal-index-integration/issues"
  },
  "homepage": "https://github.com/your-org/journal-index-integration#readme"
}
